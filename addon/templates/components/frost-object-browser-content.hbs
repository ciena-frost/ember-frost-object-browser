{{yield}}
{{#yield-slot 'controls'}}
  <div class="tools">
    <div class="sort"/>
    {{yield (block-params
      (hash
        pagination=(component
        'frost-object-browser-paginator'
          onPageChanged=(action 'onPageChanged')
        )
        expansion=(component
        'frost-object-browser-button-bar'
          detailLevel=detailLevel
          viewLevel=viewSchema
        )
      )
      this
    )}}
  </div>
{{/yield-slot}}
{{#yield-slot 'view'}}
  {{yield (block-params
    this
    (action 'onSelect')
  )}}
{{/yield-slot}}
{{#yield-slot 'selection-actions'}}
  <div class="actions">
    {{yield (block-params
      (hash
        button=(component
        'frost-object-browser-action-bar'
        )
      )
    )}}
  </div>
{{/yield-slot}}
